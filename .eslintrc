
{
  "env": {
    "jquery": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "Model": false
  },
  "ecmaFeatures": {

  },
  "rules": {
    "no-comma-dangle": 0,
    "no-cond-assign": [0, "except-parens"],
    "no-console": 0,
    "no-constant-condition": 0,
    "no-control-regex": 0,
    "no-debugger": 0,
    "no-dupe-keys": 0,
    "no-empty": 0,
    "no-empty-class": 0,
    "no-ex-assign": 0,
    "no-extra-boolean-cast": 0,
    "no-extra-parens": 0,
    "no-extra-semi": 0,
    "no-func-assign": 0,
    "no-inner-declarations": 0,
    "no-invalid-regexp": 0,
    "no-irregular-whitespace": 0,
    "no-obj-calls": 0,
    "no-regex-spaces": 0,
    "no-reserved-keys": 0,
    "no-sparse-arrays": 0,
    "no-unreachable": 0,
    "use-isnan": 0,
    "valid-jsdoc": [0, {
      "requireReturn": false
    }],
    "valid-typeof": 0,

    "block-scoped-var": 0,
    "complexity": [0, 7],
    "consistent-return": 0,
    "curly": [0, "all"],
    "default-case": 0,
    "dot-notation": [0, {"allowKeywords": true}],
    "eqeqeq": 0,
    "guard-for-in": 0,
    "no-alert": 0,
    "no-caller": 0,
    "no-div-regex": 0,
    "no-else-return": 0,
    "no-empty-label": 0,
    "no-eq-null": 0,
    "no-eval": 0,
    "no-extend-native": 0,
    "no-extra-bind": 0,
    "no-fallthrough": 0,
    "no-floating-decimal": 0,
    "no-implied-eval": 0,
    "no-iterator": 0,
    "no-labels": 0,
    "no-lone-blocks": 0,
    "no-loop-func": 0,
    "no-multi-spaces": 0,
    "no-multi-str": 0,
    "no-native-reassign": 0,
    "no-new": 0,
    "no-new-func": 0,
    "no-new-wrappers": 0,
    "no-octal": 0,
    "no-octal-escape": 0,
    "no-process-env": 0,
    "no-proto": 0,
    "no-redeclare": 0,
    "no-return-assign": 0,
    "no-script-url": 0,
    "no-self-compare": 0,
    "no-sequences": 0,
    "no-unused-expressions": 0,
    "no-void": 0,
    "no-warning-comments": 0,
    "no-with": 0,
    "radix": 0,
    "vars-on-top": 0,
    "wrap-iife": [0, "inside"],
    "yoda": [0, "never"],

    "strict": [0, "never"],

    "no-catch-shadow": 0,
    "no-delete-var": 0,
    "no-label-var": 0,
    "no-shadow": 0,
    "no-shadow-restricted-names": 0,
    "no-undef": 0,
    "no-undef-init": 0,
    "no-undefined": 0,
    "no-unused-vars": [0, {"vars": "all", "args": "after-used"}],
    "no-use-before-define": [0, "nofunc"],

    "handle-callback-err": 0,
    "no-mixed-requires": 0,
    "no-new-require": 0,
    "no-path-concat": 0,
    "no-process-exit": 0,
    "no-restricted-modules": 0,
    "no-sync": 0,

    "indent": [0, 2],
    "brace-style": [0, "1tbs", { "allowSingleLine": false }],
    "camelcase": 0,
    "comma-spacing": [0, {"before": false, "after": true}],
    "comma-style": [0, "last"],
    "consistent-this": [0, "none"],
    "eol-last": 0,
    "func-names": 0,
    "func-style": 0,
    "key-spacing": [0, {"beforeColon": false, "afterColon": true}],
    "max-nested-callbacks": [0, 3],
    "new-cap": 0,
    "new-parens": 0,
    "no-array-constructor": 0,
    "no-inline-comments": 0,
    "no-lonely-if": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-multiple-empty-lines": [0, {"max": 1}],
    "no-nested-ternary": 0,
    "no-new-object": 0,
    "no-space-before-semi": 0,
    "no-spaced-func": 0,
    "no-ternary": 0,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "no-wrap-func": 0,
    "one-var": 0,
    "operator-assignment": 0,
    "padded-blocks": [0, "never"],
    "quote-props": [0, "as-needed"],
    "quotes": [0, "single", "avoid-escape"],
    "semi": [0, "always"],
    "sort-vars": 0,
    "space-after-function-name": [0, "never"],
    "space-after-keywords": [0, "always"],
    "space-before-blocks": [0, "always"],
    "space-in-brackets": [0, "never"],
    "space-in-parens": [0, "never"],
    "space-infix-ops": 0,
    "space-return-throw-case": 0,
    "space-unary-ops": [0, { "words": true, "nonwords": false }],
    "spaced-line-comment": [0, "always"],
    "wrap-regex": 0,

    "no-var": 0,
    "generator-star": [0, "end"],

    "max-depth": [0, 3],
    "max-len": [0, 80, 2],
    "max-params": [0, 4],
    "max-statements": [0, 15],
    "no-bitwise": 0,
    "no-plusplus": 0
  }
}
